<Window
    x:Class="SoftwareEngineeringNetwork.JournalApplication.Wpf.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wpf="clr-namespace:SoftwareEngineeringNetwork.JournalApplication.Wpf"
    mc:Ignorable="d"
    Title="MainWindow"
    Height="450"
    Width="800"
    d:DataContext="{d:DesignInstance wpf:MainWindowViewModel}">
    <Grid>
        <Button
            Command="{Binding OpenRegisterUserDialogCommand}"
            Content="{Binding OpenRegisterUserDialogText}" />
    </Grid>
</Window>
